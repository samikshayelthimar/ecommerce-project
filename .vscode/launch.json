{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-BackendApplication<backend>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.angularspringbootecommerce.backend.BackendApplication",
            "projectName": "backend",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
          },
          {
            "name": "Attach Chrome",
            "type": "chrome",
            "request": "attach",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
          },
    ]
}